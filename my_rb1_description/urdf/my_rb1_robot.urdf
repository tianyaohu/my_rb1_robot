<?xml version="1.0" ?>

<robot name="rb1">
    <!-- Gazebo Material -->
    <gazebo reference="base_link">
        <material>Gazebo/Black</material>
    </gazebo>
    <gazebo reference="front_laser">
        <material>Gazebo/Black</material>
    </gazebo>
    <gazebo reference="right_wheel">
        <material>Gazebo/Grey</material>
    </gazebo>
    <gazebo reference="left_wheel">
        <material>Gazebo/Grey</material>
    </gazebo>

    <gazebo reference="front_caster">
        <material>Gazebo/Grey</material>
    </gazebo>
    <gazebo reference="back_caster">
        <material>Gazebo/Grey</material>
    </gazebo>

    <!-- Link - chassis -->
    <link name="base_link">
        <inertial>
            <mass value="20" />
            <origin xyz="0 0 0.3" rpy="0 0 0" />
            <inertia ixx="0.4625" ixy="0" ixz="0" iyy="0.4625" iyz="0" izz="0.625" />
        </inertial>

        <collision>
            <geometry>
                <cylinder radius = "0.25" length = "0.30"/>
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder radius = "0.25" length = "0.30"/>
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>
    </link>

    <!-- Laser scan -->
    <joint name="joint_laser_scan_chassis" type="fixed">
        <origin rpy="3.1415926 0 0" xyz="0.25 0 0.075" />
        <child link="front_laser" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="front_laser">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>

        <collision>
            <geometry>
                <cylinder radius = "0.05" length = "0.05"/>
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder radius = "0.05" length = "0.05"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - chassis / base_footprint -->
    <joint name="joint_base_footprint" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 -0.15" />
        <child link="base_footprint" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <!-- Link - left wheel -->
    <link name="base_footprint">
        <inertial>
            <mass value="0" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
    </link>

    <!-- Joint - chassis / left wheel -->
    <joint name="joint_left_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 -0.2 -0.15" />
        <child link="left_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <!-- Link - left wheel -->
    <link name="left_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00020833333333333337" ixy="0" ixz="0" iyy="0.00020833333333333337" iyz="0" izz="0.00031250000000000006"/>
        </inertial>

        <!-- Tires -->
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.025" radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.025" radius="0.025"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - chassis / right wheel -->
    <joint name="joint_right_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 0.2 -0.15" />
        <child link="right_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <!-- Link - left wheel -->
    <link name="right_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00020833333333333337" ixy="0" ixz="0" iyy="0.00020833333333333337" iyz="0" izz="0.00031250000000000006"/>
        </inertial>

        <!-- Tires -->
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.025" radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.025" radius="0.025"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - Front Caster Wheel -->
    <joint name="joint_front_caster" type="fixed">
        <origin rpy="0 0 0" xyz="0.1 0 -0.15" />
        <child link="front_caster" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="0.0" />
    </joint>

    <!-- Link - Front Caster Wheel -->
    <link name="front_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.0010000000000000002" ixy="0" ixz="0" iyy="0.0010000000000000002" iyz="0" izz="0.0010000000000000002"/>
        </inertial>

        <!-- Caster wheel support -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.0" />
            <geometry>
                <sphere radius="0.02" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.0" />
            <geometry>
                <sphere radius="0.02" />
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>

        <!-- Caster wheel -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -0.015" />
            <geometry>
                 <sphere radius="0.01" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.015" />
            <geometry>
                 <sphere radius="0.01" />
            </geometry>
            <material name="Grey">
                <color rgba="0.6 0.6 0.6 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - Back Caster Wheel -->
    <joint name="joint_back_caster" type="fixed">
        <origin rpy="0 0 0" xyz="-0.1 0 -0.15" />
        <child link="back_caster" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="0.0" />
    </joint>

    <!-- Link - back Caster Wheel -->
    <link name="back_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.0010000000000000002" ixy="0" ixz="0" iyy="0.0010000000000000002" iyz="0" izz="0.0010000000000000002"/>
        </inertial>

        <!-- Caster wheel support -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.0" />
            <geometry>
                <sphere radius="0.02" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.0" />
            <geometry>
                <sphere radius="0.02" />
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>

        <!-- Caster wheel -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -0.015" />
            <geometry>
                 <sphere radius="0.01" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.015" />
            <geometry>
                 <sphere radius="0.01" />
            </geometry>
            <material name="Grey">
                <color rgba="0.6 0.6 0.6 1" />
            </material>
        </visual>
    </link>

</robot>